<Window x:Class="UL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UL"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="addChildButton" Content="ADD CHILD" FontSize="40" Grid.Row="0" Click="addChildButton_Click" />
        <Button x:Name="addMotherButton" Content="ADD MOTHER" FontSize="40" Grid.Row="1" Click="addMotherButton_Click" />
        <Button x:Name="addNunnyButton" Content="ADD NUNNY" FontSize="40" Grid.Row="2" Click="addNunnyButton_Click" />
        <Button x:Name="addContractButton" Content="ADD CONTRACT" FontSize="40" Grid.Row="3" Click="addContractButton_Click" />
    </Grid>
</Window>
