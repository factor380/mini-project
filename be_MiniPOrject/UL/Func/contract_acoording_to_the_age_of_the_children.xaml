<Window x:Class="UL.contract_acoording_to_the_age_of_the_children"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UL"
        mc:Ignorable="d"
        Title="contract_acoording_to_the_age_of_the_children" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="218*"/>
        </Grid.RowDefinitions>
        <Label Content="age min or max" FontSize="9"  HorizontalAlignment="Left" Margin="10,17,0,0" VerticalAlignment="Top" Width="76"/>
        <ComboBox HorizontalAlignment="Left" Margin="98,17,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="MAX"/>
            <ComboBoxItem Content="MIN"/>
        </ComboBox>

        <Label Content="arranged" HorizontalAlignment="Left" Margin="10,2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="76"/>
        <ComboBox HorizontalAlignment="Left" Margin="98,5.926,0,0" VerticalAlignment="Top" Width="120" Grid.Row="1">
            <ComboBoxItem Content="yes"/>
            <ComboBoxItem Content="no"/>
        </ComboBox>

    </Grid>
</Window>
